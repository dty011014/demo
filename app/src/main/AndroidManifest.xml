<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.demo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.OkhttpActivity"
            android:label="网络请求okhttp" />
        <activity
            android:name=".activity.BannerActivity"
            android:label="轮播图Banner" />
        <activity
            android:name=".activity.RefreshAndLoadActivity"
            android:label="刷新/加载" />
        <activity android:name=".activity.FrameActivity"
            android:label="碎片fragment"></activity>
    </application>

</manifest>